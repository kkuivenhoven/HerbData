	<%= link_to 'Back', color_schemes_path, :class => "btn btn-light" %>
	<br>
	<br>


<table>
	<thead>
		<tr>
			<td><center><%= @color_scheme.colorOne.upcase %></center></td>
			<td><center><%= @color_scheme.colorTwo.upcase %></center></td>
