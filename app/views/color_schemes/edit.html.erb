<h1>Editing Color Scheme</h1>

<%= form_for(@color_scheme) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :colorOne %>
  <%= f.text_field :colorOne %><br>

	<%= f.label :colorTwo %>
  <%= f.text_field :colorTwo %><br>

	<%= f.label :colorThree %>
  <%= f.text_field :colorThree %><br>

	<%= f.label :rgb_one %>
  <%= f.text_field :rgb_one %><br>

	<%= f.label :rgb_two %>
  <%= f.text_field :rgb_two %><br>

	<%= f.label :rgb_three %>
  <%= f.text_field :rgb_three %><br>

	<%= f.submit "Save" %>
<% end %>
<%= link_to 'Back', color_schemes_path %>
